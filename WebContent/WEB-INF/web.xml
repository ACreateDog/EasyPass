<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>EasyPass</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PassServlet</servlet-name>
    <servlet-class>com.easypass.Controller.PassServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PassServlet</servlet-name>
    <url-pattern>/PassServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TypeSelect</servlet-name>
    <servlet-class>com.easypass.Controller.TypeSelect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TypeSelect</servlet-name>
    <url-pattern>/TypeSelect</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>skimRecord</servlet-name>
  	<servlet-class>com.easypass.Controller.SkimRecord</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>skimRecord</servlet-name>
  	<url-pattern>/skim</url-pattern>
  </servlet-mapping>
</web-app>